{
  "name": "npm-cache-benchmark",
  "description": "Benchmark npm cache vs shrinkpack vs yarn",
  "version": "2.0.0",
  "authors": [
    "Jamie Mason (https://github.com/JamieMason)",
    "Siddharth Kshetrapal (https://github.com/siddharthkp)"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "chalk": "1.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-runtime": "6.23.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "benchmark, npm-cache, npm cache, yarn"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "siddharthkp/npm-cache-benchmark",
  "scripts": {
    "benchmark": "docker run -w /benchmarks npm-cache-benchmark node index.dist.js",
    "bundle": "babel index.js -o index.dist.js",
    "build": "docker build -t npm-cache-benchmark:latest ./"
  }
}
